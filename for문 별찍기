#예제) 별찍기5
# 마름모 윗부분
for i in range(5):
    for j in range(4 - i):
        print(" ", end="")
    for j in range((i + 1) * 2 - 1):
        print("*", end="")
    print()

# 마름모 아랫부분
for i in range(3, -1, -1):
    for j in range(4 - i):
        print(" ", end="")
    for j in range((i + 1) * 2 - 1):
        print("*", end="")
    print()
